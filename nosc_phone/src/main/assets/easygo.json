{
  "easyGoVersion": "1.0",
  "client": "gov.nosc.android",
  "logicEntities": [
    {
      "head": {
        "function": "magicwindow",
        "required": "true"
      },
      "body": {
        "mode": "1",
        "Activities": [
          {
            "name": "gov.anzong.androidnga.activity.FullScreenWebViewActivity",
            "defaultFullScreen": "true"
          }
        ],
        "activityPairs": [
          {
            "from": "gov.anzong.androidnga.activity.TopicListActivity",
            "to": "*"
          },{
            "from": "gov.anzong.androidnga.activity.RecentNotificationActivity",
            "to": "*"
          },{
            "from": "gov.anzong.androidnga.activity.MainActivity",
            "to": "gov.anzong.androidnga.activity.TopicListActivity"
          },{
            "from": "gov.anzong.androidnga.activity.MainActivity",
            "to": "gov.anzong.androidnga.activity.LoginActivity"
          },{
            "from": "gov.anzong.androidnga.activity.MainActivity",
            "to": "gov.anzong.androidnga.activity.LauncherSubActivity"
          }
        ],
        "UX": {
          "supportRotationUxCompat": "false",
          "isDraggable": "true",
          "supportLock": "true",
          "supportDraggingToFullScreen": "PAD|FOLD"
        }
      }
    }
  ]
}